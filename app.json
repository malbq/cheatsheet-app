{
  "expo": {
    "name": "cheatsheet",
    "slug": "cheatsheet",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#363D49"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#363D49"
      },
      "useNextNotifications": true
    },
    "androidStatusBar": {
      "backgroundColor": "#00000000",
      "barStyle": "light-content"
    }
  }
}
